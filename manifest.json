{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "candlestick.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "js": ["background.js"],
      "matches": ["https://chartink.com/*"]
    }
  ]
}
